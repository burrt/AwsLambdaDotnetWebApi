{
  "App": {
    "Name": ".NET Web API"
  },
  "Aws": {
    "Region": "ap-southeast-2",
    "SecretsManager": {
      "SecretName": "secretName"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Enrichers.Sensitive"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  }
}
